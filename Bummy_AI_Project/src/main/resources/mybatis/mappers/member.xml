<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.bummy.web.dao.MemberDAO">
	<insert id="signup" parameterType="memberVO" >
		<![CDATA[
			insert into user(user_id,user_pw,user_name,user_phone,user_email,user_belong,user_type) 
			values(#{user_id},#{user_pw},#{user_name},#{user_phone},#{user_email},#{user_belong},#{user_type})
		]]>
	</insert>
	
</mapper> 